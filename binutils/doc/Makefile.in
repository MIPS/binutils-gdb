# Man page generation from texinfo
addr2line.1:	$(BINUTILS_TEXI)
	touch $@
	-$(TEXI2POD) $(MANCONF) -Daddr2line < $< > $@.pod
	-($(POD2MAN) $@.pod | sed -e '/^.if n .na/d' > $@.T$$$$ && \
		mv -f $@.T$$$$ $@) || (rm -f $@.T$$$$ && exit 1)
	rm -f $@.pod

